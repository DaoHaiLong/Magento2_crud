<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="aht_ahttest_table" resource="default" engine="innodb" comment="aht_ahttest_table Table">
		<column xsi:type="int" name="table_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="table_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		<column name="age" nullable="true" xsi:type="int" comment="age" identity="false"/>
	</table>
</schema>